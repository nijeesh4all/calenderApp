<div id="<%= dom_id event %>">
  <p>
    <strong>Title:</strong>
    <%= event.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= event.description %>
  </p>

  <p>
    <strong>Start date time:</strong>
    <%= event.start_date_time %>
  </p>

  <p>
    <strong>End date time:</strong>
    <%= event.end_date_time %>
  </p>

</div>
